            <div class="left-img">
            </div>
           </div>
        </div>
    <!--  左侧边栏 end  -->
    <!--  右侧边栏    -->
        <div class="right-side">
            <footer>
              <div class="footer-l">
                <div class="avail_room_title">30天内可用房源一览表</div>
                <table id = "avail_room_table">
                  <!-- 0-10天 -->                
                  <tr>
                    <th>日期</th>
                    <% for(var i = 0; i < numbers.length&&i<10; i++){%>
                        <td><%=  numbers[i].days %></td>
                    <%}%>
                  </tr>

                  <tr>
                    <th>单人房</th>
                    <% for(var i = 0; i < numbers.length&&i<10; i++){%>
                        <td><%= numbers[i].singleRoom %></td>
                    <%}%>    
                  </tr>
                  <tr>
                    <th>双人房</th>
                    <% for(var i = 0; i < numbers.length&&i<10; i++){%>
                        <td><%= numbers[i].doubleRoom %></td>
                    <%}%>    
                  </tr>
                  <tr>
                    <th>大房</th>
                    <% for(var i = 0; i < numbers.length&&i<10; i++){%>
                        <td><%= numbers[i].bigRoom %></td>
                    <%}%>    
                  </tr>

                  <!-- 10-20天 --> 
                  <tr>
                    <th>日期</th>
                    <% for(var i = 10; i < numbers.length&&i<20; i++){%>
                        <td><%= numbers[i].days %></td>
                    <%}%>
                  </tr>

                  <tr>
                    <th>单人房</th>
                    <% for(var i = 10; i < numbers.length&&i<20; i++){%>
                        <td><%= numbers[i].singleRoom %></td>
                    <%}%>    
                  </tr>
                  <tr>
                    <th>双人房</th>
