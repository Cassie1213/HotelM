                        <td><%= numbers[i].doubleRoom %></td>
                    <%}%>    
                  </tr>
                  <tr>
                    <th>大房</th>
                    <% for(var i = 10; i < numbers.length&&i<20; i++){%>
                        <td><%= numbers[i].bigRoom %></td>
                    <%}%>    
                  </tr>

                  <!-- 20-30天 --> 
                  <tr>
                    <th>日期</th>
                    <% for(var i = 20; i < numbers.length&&i<30; i++){%>
                        <td><%=  numbers[i].days %></td>
                    <%}%>
                  </tr>

                  <tr>
                    <th>单人房</th>
                    <% for(var i = 20; i < numbers.length&&i<30; i++){%>
